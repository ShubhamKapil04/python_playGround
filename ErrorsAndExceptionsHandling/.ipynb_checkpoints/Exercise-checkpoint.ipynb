    "    while True:\n",
    "        try: \n",
    "            number = int(input(\"Provide the number: \"))\n",
    "            print(number ** 2)\n",
    "        except:\n",
    "            print(\"Wops!! Provide a number\")\n",
    "            continue\n",
    "        else:\n",
    "            print(\"Thank You!!\")\n",
    "            break"
